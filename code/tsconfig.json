{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "incremental": true,
    "paths": {
      "src/*": ["src/*"]
    },
    "skipLibCheck": false,
    "strict": true,
    "strictPropertyInitialization": false,
    "tsBuildInfoFile": ".tsbuildinfo"
  }
}
